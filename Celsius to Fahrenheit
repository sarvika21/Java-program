public class CelsiusToFahrenheit {
    public static void main(String[] args) {
        double celsius = 25; // Example temperature in Celsius

        // Formula: F = (C × 9/5) + 32
        double fahrenheit = (celsius * 9 / 5) + 32;

        System.out.println(celsius + "°C = " + fahrenheit + "°F");
    }
}
